---
const companyItems = ["Creator Awards", "Creative Ideas", "Residences"];
---

<div class="flex flex-col">
  <h3 class="font-bold text-[20px] md:text-[24px] mb-4">Impact</h3>
  <ul class="font-book">
    {
      companyItems.map((item, index) => (
        <li>
          <a href="#">{item}</a>
        </li>
      ))
    }
  </ul>
</div>
